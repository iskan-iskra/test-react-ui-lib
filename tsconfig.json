{
  "compilerOptions": {
     "moduleResolution": "node",
    "target": "ES5",
    "module": "ESNext",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}